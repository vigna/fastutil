					} else {
						n = remaining;
						pos = 0;
					}
					last = pos;
					return n;
				}
				@Override
				public long skip(long n) {
					if (n < 0) throw new IllegalArgumentException("Argument must be nonnegative: " + n);
					final long remaining = size - pos;
					if (n < remaining) {
						pos += n;
					} else {
						n = remaining;
						pos = size;
					}
					last = pos - 1;
					return n;
