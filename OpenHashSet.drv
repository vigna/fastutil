		public KEY_GENERIC_TYPE PREV_KEY() {
			if ( ! hasPrevious() ) throw new NoSuchElementException();

			curr = prev;
			prev = link[ curr ] ^ next;
			next = curr;

			index--;

			return key[ curr ];
