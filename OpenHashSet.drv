	public boolean trim( final int n ) {
		int l = Arrays.binarySearch( PRIMES, (int)( Math.min( Integer.MAX_VALUE - 1, Math.max( n, count ) / f ) ) + 1 );
		if ( l < 0 ) l = -l - 1;
