			next = first;
		}

		SetIterator( KEY_GENERIC_TYPE from ) {
			if ( KEY_EQUALS( key[ last ], from ) ) {
				prev = last;
				index = count;
			}
			else {
				if ( ! contains( from ) ) throw new IllegalArgumentException( "The key " + from + " does not belong to this set." );
				next = first;
				KEY_GENERIC_TYPE k;
				do k = NEXT_KEY(); while( ! KEY_EQUALS( k, from ) );
